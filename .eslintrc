{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "airbnb",
    "rules": {
        "semi": 2,
        "comma-dangle": 0,
        "max-len": 0,
        "react/forbid-prop-types": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-no-bind": 0,
        "jsx-a11y/anchor-has-content": 0,
        "jsx-a11y/no-static-element-interactions": 0
    }
}